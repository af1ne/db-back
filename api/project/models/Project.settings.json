    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "date": {
      "type": "date"
    },
    "link": {
      "type": "text"
    },
    "technologies": {
      "type": "text"
    }
  }
}